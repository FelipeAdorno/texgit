<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<txg:Texgit xmlns:txg="http://gilmatryx.googlepages.com/Texgit"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://gilmatryx.googlepages.com/Texgit TexgitSchema.xsd">

	<FlatFile>
		<layout>
			<name>Layout teste</name>
			<version>Versão primária</version>
		</layout>
		<GroupOfRecords>
			<Record name="X-RECORD" repeatable="true">
				<GroupOfFields>
				
					<IdType name="header" type="INTEGER" value="01"
						length="2" position="1" />
						
					<Field name="nome" length="30" />
					<Field padding="ZERO_LEFT" name="cpf" length="14" />
					<Field type="DATE" name="nascimento" length="15">
						<Filler sideToFill="LEFT" padding="DC " />
						<Formatter type="DATE" format="dd-MM-yyyy" />
					</Field>
					<Field type="LONG" padding="ZERO_LEFT" name="compra"
						length="9" />
					<Field type="BIGDECIMAL" padding="ZERO_LEFT"
						name="valor" length="13" format="DECIMAL_DDDD" />
					
					<SequencialNumber name="sequencia" type="LONG"
						length="6" position="6" />
						
				</GroupOfFields>

				<GroupOfInnerRecords>
					<Record name="X" >
						<GroupOfFields>
						
							<IdType name="aninhado" type="INTEGER" value="01" length="2" position="1" />
						
							<Field name="nome" length="30" />
							<Field padding="ZERO_LEFT" name="cpf"
								length="14" />
						
							<SequencialNumber name="sequencia" type="LONG"
							length="3" position="3" />
							
						</GroupOfFields>
					</Record>
				</GroupOfInnerRecords>

			</Record>

		</GroupOfRecords>
	</FlatFile>
</txg:Texgit>
